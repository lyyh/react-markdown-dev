{
  "name": "react-markdown",
  "description": "Renders Markdown as React components",
  "version": "3.0.1",
  "keywords": [
    "markdown",
    "react",
    "react-component"
  ],
  "main": "lib/react-markdown.js",
  "scripts": {
    "build": "npm run clean && npm run compile && NODE_ENV=production webpack -p && npm run build:demo",
    "build:demo": "NODE_ENV=production webpack -p --config webpack.config.demo.js",
    "start": "webpack-dev-server --inline --hot --color --compress --port 3000 --config webpack.dev.config.js ",
    "clean": "rimraf lib/",
    "compile": "babel --copy-files -d lib/ src/",
    "deploy": "gh-pages-deploy",
    "posttest": "eslint .",
    "prepublishOnly": "npm run qa",
    "qa": "npm test && npm run build",
    "test": "jest --coverage",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rexxars/react-markdown.git"
  },
  "author": "Espen Hovlandsdal <espen@hovlandsdal.com>",
  "license": "MIT",
  "gh-pages-deploy": {
    "noprompt": true,
    "prep": [
      "build"
    ],
    "staticpath": "demo/dist"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "remark-parse": "^4.0.0",
    "unified": "^6.1.5",
    "unist-util-visit": "^1.1.3",
    "xtend": "^4.0.1",
    "react-markdown": "^3.0.2",
    "codemirror": "^5.32.0",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "file-loader": "^0.10.1",
    "less-loader": "^2.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-sanity": "^3.1.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages-deploy": "^0.4.2",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^16.0.0",
    "remark-breaks": "^1.0.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-hot-middleware": "^2.10.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**.js"
    ]
  }
}
